{
  "name": "vr-rpg-client",
  "version": "1.0.0",
  "description": "VR-RPG-Client is the client side of the VR-RPG project",
  "dependencies": {
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-router": "^4.0.15",
    "@types/react-router-dom": "^4.0.7",
    "babylonjs": "^3.0.7",
    "protobufjs": "^6.8.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.2.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "http-server": "^0.10.0",
    "raven-js": "^3.17.0",
    "source-map-loader": "^0.2.1",
    "typescript": "^2.4.1",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.8.2"
  },
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development webpack",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "pbjs": "pbjs -t static-module -o src/proto/compiled.js proto/**/*.proto",
    "pbts": "pbts -o src/proto/compiled.d.ts src/proto/compiled.js",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --port 9000 --inline --content-base .",
    "start:prod": "cross-env NODE_ENV=production webpack-dev-server --port 9000 --inline --content-base ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/vrrpg/vr-rpg-client.git"
  },
  "keywords": [],
  "author": "Gyorgy Bucsek - gyorgy.bucsek@gmail.com",
